!! URxvt Appearance
URxvt*scrollBar: false
URxvt*scrollBar_right: false


!! Copy Paste
URxvt*iso14755: false
URxvt*iso14755_52: false‚Üê
URxvt*copyCommand: xclip -i -selection clipboard
URxvt*pasteCommand: xclip -o -selection clipboard
URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard


URxvt*perl-ext-common: default,matcher,selection-to-clipboard
URxvt*url-launcher: firefox
URxvt*underlineURLs: true
URxvt*matcher.button: 1
!!URxvt*urlButton: 1


URxvt*font: xft:Hack-Regular:pixelsize=13,xft:Symbola
URxvt*boldFont: xft:Hack-Bold:pixelsize=13:weight=bold,xft:Symbola
URxvt*italicFont: xft:Hack-RegularOblique:pixelsize=13:slant=italic,xft:Symbola


URxvt*depth: 32
URxvt*background: [90]#161616

!! Fake Transparent (set background image as background)
!!URxvt*transparent: true
